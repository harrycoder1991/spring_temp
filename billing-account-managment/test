CREATE TABLE DIRECT_BILL_PLAN (
    ID INT PRIMARY KEY IDENTITY(1,1),
    HQ_STATE VARCHAR(255),
    BIL_TYPE_CD VARCHAR(255),
    BIL_CLASS_CD VARCHAR(255),
    POLICY_PLAN VARCHAR(255),
    EFT_PLAN VARCHAR(255),
    POLICY_PKG_TYPE VARCHAR(255),
    POLICY_PREFIX VARCHAR(255),
    PMT_PLAN VARCHAR(255),
    COLLECT_METH VARCHAR(255),
    PLAN_EFF_DT DATE,
    PLAN_EXP_DT DATE
);
